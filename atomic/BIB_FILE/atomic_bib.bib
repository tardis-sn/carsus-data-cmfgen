	keywords = {atomic data, FeIV, oscillator strengths},
	month = mar,
	pages = {779-785},
	title = {{Atomic data from the iron project. LXI. Radiative E1, E2, E3, and M1 transition probabilities for Fe IV}},
	volume = 448,
